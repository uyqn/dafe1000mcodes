%Differensiallikning:
F = @(x, y) -0.1*(y - 15 + 

%Initial betingelse:
x0 = 0;
y0 = 1;

%Iterer over intervallet:
I = [0, 24];

%Steglengde
h = 0.1;

%Magic:
xn = x0;
yn = y0;
%plot løsningen
syms x
f = 3*cos(x) - 2*(cos(x))^2;

fplot(x, f, I)

hold on
while xn < max(I)
    plot(xn, yn, "ro")
    axis([x0 max(I) -6 2])
    xn = xn + h;
    yn = yn + h*F(xn, yn);
    pause(0.1)
end
hold off